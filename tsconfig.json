{
  "display": "Node 20",

  "compilerOptions": {
    "lib": ["es2020"],
    "module": "CommonJS",
    "target": "es2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true
  },

  "include": ["enums/*", "interfaces/*", "index.ts"]
}